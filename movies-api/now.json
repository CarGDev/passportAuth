{
  "name": "platzivideo",
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
  "env": {
    "DB_USER": "@user_name_passport",
    "DB_PASSWORD": "@password_db",
    "DB_HOST": "@host_name",
    "DB_NAME": "@db_name_passport"
  }
}
